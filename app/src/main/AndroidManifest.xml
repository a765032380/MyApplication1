<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gll.myapplication">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".app.MyApp"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.personal.AccountActivity"> </activity>
        <activity android:name=".activity.personal.CachingActivity"> </activity>
        <activity android:name=".activity.personal.CollectionActivity"> </activity>
        <activity android:name=".activity.personal.HelpActivity"> </activity>
        <activity android:name=".activity.personal.HistoryActivity"> </activity>
        <activity android:name=".activity.personal.PasswordActivity"> </activity>
        <activity android:name=".activity.personal.SettingActivity"> </activity>
        <activity android:name=".activity.personal.VoteActivity"> </activity>
        <activity android:name=".activity.personal.WalletActivity"> </activity>
        <activity android:name=".activity.VideoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
        </activity>
        <activity android:name=".activity.RecycleActivity"> </activity>
        <activity android:name=".activity.ImageActivity"> </activity>
        <activity android:name=".activity.IdiomStoryActivity"> </activity>
        <activity android:name=".activity.HealthActivity"> </activity>
        <activity android:name=".activity.LoginActivity"> </activity>



    </application>

</manifest>